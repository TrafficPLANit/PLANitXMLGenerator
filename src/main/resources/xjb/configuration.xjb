<jaxb:bindings version="2.1"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<!-- Only refer to one input XSD file, JAXB looks through all XSD files 
		in the directory with default namespace -->
	<jaxb:bindings	schemaLocation="../xsd/macroscopicinput.xsd">
		<jaxb:schemaBindings>
			<jaxb:package name="org.planit.xml.generated" />
		</jaxb:schemaBindings>
	</jaxb:bindings>

</jaxb:bindings>